{
  "$schema": "https://docs.tolgee.io/cli-schema.json",
  "projectId": 27621,
  "format": "JSON_I18NEXT",
  "patterns": ["./src/**/*.ts?(x)"],
  "extractor": "./extractor.ts",
  "defaultNamespace": "landing-page",
  "push": {
    "filesTemplate": "./src/i18n/{namespace}/{languageTag}.json",
    "language": ["en", "fi", "sv"],
    "namespaces": ["landing-page", "common"],
    "forceMode": "KEEP"
  },
  "pull": {
    "path": "./src/i18n/",
    "namespaces": ["landing-page", "common"]
  }
}
